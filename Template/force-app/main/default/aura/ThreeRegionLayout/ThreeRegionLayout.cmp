<aura:component implements="lightning:appHomeTemplate" description="Custom Three-Column Layout">
    
    <aura:attribute name="header" type="Aura.Component[]" />
    
    <aura:attribute name="one_region" type="Aura.Component[]" />

    
    <aura:attribute name="footer" type="Aura.Component[]" />
    
    <div>
    	<lightning:layout>
        	<lightning:layoutItem size="12" class="">
            	{!v.header}
            </lightning:layoutItem>
        </lightning:layout>
    
    	<lightning:layout>
        	<lightning:layoutItem size="12" class="">
            	{!v.one_region}
            </lightning:layoutItem>
        
        </lightning:layout>
    
    	<lightning:layout>
        	<lightning:layoutItem size="12" class="">
            	{!v.footer}
            </lightning:layoutItem>
        </lightning:layout>
    </div>
	
</aura:component>